{
  "buildCommand": "echo 'No build needed'",
  "framework": null,
  "devCommand": null,
  "installCommand": "echo 'No install needed'",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.tsx"
    },
    {
      "source": "/login",
      "destination": "/login.tsx"
    },
    {
      "source": "/signup",
      "destination": "/signup.tsx"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.tsx"
    },
    {
      "source": "/forgot-password",
      "destination": "/forgot-password.tsx"
    },
    {
      "source": "/reset-password",
      "destination": "/reset-password.tsx"
    },
    {
      "source": "/(.*)",
      "destination": "/index.tsx"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
